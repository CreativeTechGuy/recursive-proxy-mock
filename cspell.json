{
    "version": "0.2",
    "words": [
        "bundlephobia",
        "chainable",
        "commitlint",
        "corejs",
        "eqeqeq",
        "filesize",
        "iife",
        "lcov",
        "loglevel",
        "ttypescript",
        "webgl"
    ],
    "ignoreRegExpList": ["\\(#.*\\)"],
    "ignorePaths": ["package-lock.json", "package.json"],
    "useGitignore": true,
    "validateDirectives": true,
    "cache": {
        "useCache": true,
        "cacheLocation": "./node_modules/.cache/cspell/.cspell"
    }
}
